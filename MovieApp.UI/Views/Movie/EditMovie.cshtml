@model MyMovieApp.Entity.MovieModel;
@{
    ViewData["Title"] = "EditMovie";
}

<h1>EditMovie</h1>

<form method="post">
    <div class="form-group">
        <label>
            Movie Title
        </label>
        <input type="text" class="form-control" asp-for="MovieTitle"/>
    </div>

    <div class="form-group">
        <label>
            Description
        </label>
        <input type="text" class="form-control" asp-for="MovieDescription" />
    </div>

    <div class="form-group">
        <label>
            Movie Type
        </label>
        <input type="text" class="form-control" asp-for="MovieType"/>
    </div>

    

    

    <div class="form-group">
        <label>
            Movie Language
        </label>
        <input type="text" class="form-control" asp-for="MovieLanguage"/>
    </div>

    <input type="submit" value="Update" class="btn btn-primary" />
    <input type="button" value="Reset" class="btn btn-primary" />
</form>